{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Nifi Manager",
      "type": "node",
      "request": "attach",
      "remoteRoot": "/app/src/",
      "port": 9229,
      "address": "localhost",
      "localRoot": "${workspaceFolder}/nifi-management-server/src/",
      "protocol": "inspector",
      "restart": true
    },
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/app.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
